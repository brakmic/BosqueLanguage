namespace NSMain;

function complex_switch(): Int {
  let x = {a = 2, b = 1};
  let result = switch(x) {
    case {a=2, b=1} => 99
    case let {a=1, b=2} => x.b
    case _ => -1
  };
  return result;
}

entrypoint function main(): Int {
  let a = 10;
  let result = switch(a) {
    case 1 => 1
    case 0 => 0
    case _ => -1
  };
  return result;
  // return complex_switch();
}